{
  "version" : 1,
  "taskType" : "CLASSIC",
  "turnpoints" : [<% for (var i = 0; i < turnpoints.length; i++ ) { %>{
       "radius" : <%- turnpoints[i].radius %>.0,
       "waypoint" : {
         "lon" : <%- turnpoints[i].y %>,
         "lat" : <%- turnpoints[i].x %>,
         "altSmoothed" : <%- turnpoints[i].z %>.0,
         "name" : "<%-turnpoints[i].name %>",
         "description" : "<%- turnpoints[i].id %>"
       },
       "type" : "<%- turnpoints[i].type %>"
     }<% if (i < turnpoints.length - 1) { %>,<% } %><% } %>
  ],
  "sss" : {
    "type" : "<%- xcInfo.type.toUpperCase() %>",
    "direction" : "<%- xcInfo.direction %>",
    "timeGates" : ["<%- xcInfo.timeGates %>Z"]
  },  
  "goal" : {
    "type" : "<%- xcInfo.goalType.toUpperCase() %>",
    "deadline" : "<%- xcInfo.deadline %>Z"
  }
}
