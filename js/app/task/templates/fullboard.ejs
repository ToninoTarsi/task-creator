<div id="task-config" class="modal fade" role="dialog" aria-labelledby="">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div id="fullboard-header" class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="turnpoint-config-label">Task Configuration</h4>
      </div>
      <div id="fullboard-content" class="modal-body">
        <form action="#" autocomplete="off" id="task-form-config">
          <div id="fullboard-type" class="left"> Type 
            <select id="fullboard-type-select" autocomplete="off" name="type">
            </select>
          </div>
          <div id="fullboard-num" class="right">Task 
            <select id="fullboard-num-select" autocomplete="off" name="num">
            </select>
          </div>
          <div id="fullboard-takeoff" class="left">take off
            <ul>
            </ul>
          </div>
          <div id="fullboard-right-infos" class="right">
            <div id="fullboard-date" class="">Date <span></span></div>
            <div id="fullboard-turn" class="">
              <span id="fullboard-arrow"></span>
              <span id="fullboard-turn-word"></span>
            </div>
          </div>
          <div id="fullboard-times" class="right">
            <div id="fullboard-window" class="">window 
              <span id="window-open"></span>
              <span id="window-close"></span>
            </div>
          <div id="fullboard-task-deadline" class="">task deadline
            <ul>
              <li>
                <span id="ess-time"> ESS </span>
                <span class="value"></span>
              </li>
              <li>
                <span id="goal-time">Goal </span>
                <span class="value"></span>
              </li>
            </ul>
          </div>
        </div>
        <div id="fullboard-task" class="left">
          <span id="fullboard-turnpoint">turnpoints 
            <ul>
            </ul>
          </span>
          <span id="fullboard-end-of-speed-section">end of speed section
            <ul>
            </ul>
          </span>
          <span id="fullboard-goal">goal 
            <ul>
            </ul>
          </span>
        </div>
        <div id='fullboard-task-distance' class='right'>distance <span>Km</span></div>
      </form>
      <div id="export-task" class="export-full-task form-button"> Export Task !</div>
      <div id="edit-task" class="edit-full-task form-button"> Edit Task !</div>
      <div id="delete-task" class="delete-full-task form-button"> Delete Task !</div>
      <span id="fullboard-xm">XM was here !</span>
      </div>
    </div>
  </div>
</div>
